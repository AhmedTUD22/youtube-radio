{
  "version": 2,
  "name": "youtube-radio",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
